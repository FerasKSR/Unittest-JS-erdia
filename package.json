{
  "name": "erdia",
  "version": "0.2.2",
  "description": "create mermaid.js er diagram using typeorm",
  "main": "dist/erdia.js",
  "types": "dist/erdia.d.ts",
  "scripts": {
    "dev": "ts-node -r tsconfig-paths/register src/erdia.ts",
    "debug": "node -r ts-node/register -r tsconfig-paths/register --inspect-brk app.ts",
    "prebuild": "npm run clean",
    "build": "tsc",
    "clean": "rimraf ./dist",
    "webpack:prod": "cross-env NODE_ENV=production webpack --config webpack.config.prod.cjs",
    "webpack:dev": "cross-env NODE_ENV=production webpack --config webpack.config.dev.cjs",
    "prepub": "npm run clean && npm run webpack:prod",
    "pub": "npm publish --registry http://localhost:8901 --force",
    "prepub:prod": "npm run clean && npm run webpack:prod",
    "pub:prod": "npm publish",
    "lint": "eslint --ext ts,tsx ./src/*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imjuni/er-doc-writer.git"
  },
  "files": [
    "dist"
  ],
  "bin": {
    "erdia": "./dist/erdia.js"
  },
  "author": "ByungJoon Lee",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/imjuni/er-doc-writer/issues"
  },
  "homepage": "https://github.com/imjuni/er-doc-writer#readme",
  "devDependencies": {
    "@jest/test-sequencer": "^27.4.6",
    "@tsconfig/node16": "^1.0.2",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.8",
    "@types/source-map-support": "^0.5.4",
    "@types/yargs": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/eslint-plugin-tslint": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "better-sqlite3": "^7.5.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.7.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsdoc": "^37.6.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.4.7",
    "prettier": "^2.5.1",
    "prettier-eslint": "^13.0.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "tslint": "^6.1.3",
    "types-webpack-bundler": "^1.0.2",
    "typescript": "^4.5.4",
    "webpack": "^5.66.0",
    "webpack-cli": "^4.9.1",
    "webpack-node-externals": "^3.0.0",
    "webpackbar": "^5.0.2"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "my-easy-fp": "^0.10.0",
    "source-map-support": "^0.5.21",
    "typeorm": "^0.2.41",
    "yargs": "^17.3.1"
  }
}
